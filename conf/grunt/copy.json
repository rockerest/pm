{
    "i18n": {
        "files": [
            {
                "expand": true,
                "cwd": "src/js/nls/",
                "src": ["**"],
                "dest": "build/js/nls/"
            }
        ]
    },
    "images": {
        "files": [
            {
                "expand": true,
                "cwd":  "src/images/",
                "src": ["**"],
                "dest": "build/img/"
            }
        ]
    },
    "vendor": {
        "files": [
            {
                "expand": true,
                "cwd": "vendor/font-awesome/fonts/",
                "src": ["**"],
                "dest": "build/vendor/font-awesome/"
            }
        ]
    }
}
